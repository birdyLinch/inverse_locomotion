            <joint name='right_hip_y' type='hinge' pos='0 0 0' axis='0 0.34 0.93' range='5.2 5.3' damping='5' stiffness='20' armature='0.32' />
            <joint name='right_hip_x' type='hinge' pos='0 0 0' axis='0 0.93 -0.34' range='13 14' damping='5' stiffness='10' armature='0.4' />

